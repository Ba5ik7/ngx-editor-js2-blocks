{
  "name": "ngx-editor-js2-blocks",
  "version": "19.0.0",
  "scripts": {
    "watch-demo": "ng serve demo",
    "watch-blockquotes": "ng build ngx-editor-js2-blockquotes --watch --configuration development",
    "watch-codemirror": "ng build ngx-editor-js2-codemirror --watch --configuration development",
    "watch-image": "ng build ngx-editor-js2-image --watch --configuration development",
    "watch-pop-quiz": "ng build ngx-editor-js2-pop-quiz --watch --configuration development",
    "watch-mfe-loader": "ng build ngx-editor-js2-mfe-loader --watch --configuration development",
    "watch-mermaidjs": "ng build ngx-editor-js2-mermaidjs --watch --configuration development",
    "build-all-libraries": "npm-run-all --parallel build-blockquotes build-image build-codemirror build-pop-quiz build-mfe-loader build-mermaidjs",
    "build-demo": "ng build demo --configuration development --base-href \"/ngx-editor-js2-blocks/\"",
    "build-blockquotes": "ng build ngx-editor-js2-blockquotes --configuration production",
    "build-image": "ng build ngx-editor-js2-image --configuration production",
    "build-codemirror": "ng build ngx-editor-js2-codemirror --configuration production",
    "build-pop-quiz": "ng build ngx-editor-js2-pop-quiz --configuration production",
    "build-mfe-loader": "ng build ngx-editor-js2-mfe-loader --configuration production",
    "build-mermaidjs": "ng build ngx-editor-js2-mermaidjs --configuration production",
    "deployDemo": "npx angular-cli-ghpages --dir=dist/demo",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": false,
  "dependencies": {
    "@angular-architects/module-federation": "^19.0.3",
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.1.3",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.1.3",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "mermaid": "^11.6.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@ctrl/ngx-codemirror": "^7.0.0",
    "@tmdjr/ngx-editor-js2": "^0.0.9",
    "@types/codemirror": "5.60.13",
    "@types/jasmine": "~5.1.0",
    "codemirror": "5.65.9",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.1.0",
    "ngx-build-plus": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.6.2"
  }
}
